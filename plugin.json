{
	"id": "nodebb-plugin-sso-wechat-web",
	"name": "NodeBB Wechat SSO Login (QRCODE)",
	"description": "NodeBB Plugin that allows users to login/register via their Wechat account.",
	"url": "https://github.com/a632079/nodebb-plugin-sso-wechat-web",
	"library": "./library.js",
	"hooks": [
		{
			"hook": "static:app.load",
			"method": "init"
		},
		{
			"hook": "filter:auth.init",
			"method": "getStrategy"
		},
		{
			"hook": "filter:user.delete",
			"method": "deleteUserData"
		},
		{
			"hook": "filter:auth.list",
			"method": "getAssociation"
		},
		{
			"hook": "filter:admin.header.build",
			"method": "addMenuItem"
		},
		{
			"hook": "filter:user.listPictures",
			"method": "list"
		},
		{
			"hook": "filter:user.getPicture",
			"method": "get"
		},
		{
			"hook": "filter:register.interstitial",
			"method": "prepareInterstitial"
		}
	],
	"staticDirs": {
		"static": "./static"
	},
	"templates": "static/templates",
	"less": [
		"static/less/style.less"
	],
	"acpScripts": [
		"static/lib/admin.js"
	]
}